{
  "version": 2,
  "builds": [
    {
      "src": "task/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": ["templates/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "task/app.py"
    }
  ]
}
